#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules/nuxt/bin/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules/nuxt/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules/nuxt/bin/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules/nuxt/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/nuxt@3.10.0_sass@1.70.0_vite@5.0.12/node_modules:/home/natethegreat/Portfolio/TheCalculatorGuyDEV/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
